Section:NewKeybind("heavy punch ", "change it to whatever u like just press it and press the key you want it on", Enum.KeyCode.Z, function()
local args = {
    [1] = 0.4,
    [2] = 0.1,
    [3] = 1
}

game:GetService("ReplicatedStorage").Events.Punch:FireServer(unpack(args))

Section:NewKeybind("heavy punch ", "change it to whatever u like just press it and press the key you want it on", Enum.KeyCode.Z, function()
local args = {
    [1] = 0.4,
    [2] = 0.1,
    [3] = 1
}

game:GetService("ReplicatedStorage").Events.Punch:FireServer(unpack(args))

end)
end)


local Tab = Window:NewTab("anti grab")
local Section = Tab:NewSection("leave to unequip ")

Section:NewButton("anti grab", "cant unequip", function()
 while(true)do
              local evento = game:GetService("ReplicatedStorage").Events.ToggleTelekinesis;
              if game.Players.LocalPlayer.Character then
                evento:InvokeServer(game.Workspace.Camera.CFrame.LookVector, false, game.Players.LocalPlayer.Character); 
              end
              wait()   
              end
end)
