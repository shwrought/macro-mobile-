local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("age of heroes fight helper", "GrapeTheme")

local Tab = Window:NewTab("main")

local Section = Tab:NewSection("tools to help you fight")





Section:NewButton("inf yield", "opens inf yield", function()
  loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

Section:NewKeybind("normal punch ", "to change it press the button and press the key you want it to ", Enum.KeyCode.G, function()
    local args = {



    [1] = 0,
    [2] = 0.1,
    [3] = 1
}

game:GetService("ReplicatedStorage").Events.Punch:FireServer(unpack(args))

end)





local args = {



    [1] = 0.1,
    [2] = 0.1,
    [3] = 1
}

game:GetService("ReplicatedStorage").Events.Punch:FireServer(unpack(args))


Section:NewKeybind("heavy punch ", "change it to whatever u like just press it and press the key you want it on", Enum.KeyCode.F, function()
local args = {
    [1] = 0.4,
    [2] = 0.1,
    [3] = 1
}

game:GetService("ReplicatedStorage").Events.Punch:FireServer(unpack(args))


end)




Section:NewKeybind("K to open/close", "opens/close it with K", Enum.KeyCode.K, function()
    Library:ToggleUI()
end)

--aap

