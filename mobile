   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-392.009216, 672.115356, 267.762726, -0.0532506332, -0.983429313, 0.173295051, -2.4716293e-08, 0.173541278, 0.984826624, -0.998581171, 0.0524426326, -0.0092412075)
end)


